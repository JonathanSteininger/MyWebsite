<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - MyActualWebsite</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/MyActualWebsite.styles.css" asp-append-version="true" />
    <link rel="icon" href="~/content/images/logo.png" />
</head>
<body class="bg-primary">
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-dark bg-primary border-primary border-bottom border-2 mb-3 ">
            <div class="container-fluid">
                <a class="navbar-brand"  asp-area="" asp-controller="Home" asp-action="Index">
                    <img src="~/Content/Images/logo.png" />
                </a>
                <button class="border-white navbar-toggler border-0 " type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <img src="~/Content/Images/ham.png" class="navbar-toggler-img"/>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1 ">
                        <li class="nav-item">
                            <a class="nav-link text-light" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-light" asp-area="" asp-controller="Home" asp-action="Portfolio">Portfolio</a>
                        </li>

                        <li class="nav-item">
                            <a class="nav-link text-light" asp-area="" asp-controller="Home" asp-action="Experience">Experience</a>
                        </li>
                        
                        <li class="nav-item">
                            <a class="nav-link text-light" asp-area="" asp-controller="Home" asp-action="Contacts">Contacts</a>
                        </li>
                        <partial name="_AdminNavPartial"/>
                    </ul>
                    <partial name="_LoginPartial" />
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top border-primary border-2 footer text-muted bg-primary">
        <div class="footer-nav-box">
            <ul class="footer-nav border-end border-start border-1 border-secondary-darker">
                <li class="footer-nav-item">
                    <a class="footer-nav-link" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                </li>
                <li class="footer-nav-item">
                    <a class="footer-nav-link" asp-area="" asp-controller="Home" asp-action="Portfolio">Portfolio</a>
                </li>
            </ul>
            
            <ul class="footer-nav border-end border-start border-1 border-secondary-darker">
                <li class="footer-nav-item">
                    <a class="footer-nav-link" asp-area="" asp-controller="Home" asp-action="Experience">Experience</a>
                </li>

                <li class="footer-nav-item">
                    <a class="footer-nav-link" asp-area="" asp-controller="Home" asp-action="Contacts">Contacts</a>
                </li>
            </ul>
            <ul class="footer-nav border-end border-start border-1 border-secondary-darker">
                <partial name="_loginFooterPartial" />

            </ul>
            <ul class="footer-nav border-end border-start border-1 border-secondary-darker">
                <li class="footer-nav-item">
                    <a class="footer-nav-link" asp-area="" asp-controller="Home" asp-action="FAQ">FAQs</a>
                </li>
            </ul>
        </div>
        <div class="container footer-copyright text-center">
            Jonathan Steininger &copy; 2023 - Jonathan-Steininger
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
<style type="text/css">
    
    
</style>
