@model List<MyActualWebsite.Models.Tag>
@if (Model.Count == 0) return;
<div class="bg-primary m-2 p-2 border-radius-primary flex-grow-1" style="">

    <p class="h5 text-color-heading my-auto mx-2">
        @ViewData["ListName"]
    </p>
    <div class="d-flex flex-wrap align-content-center" style="">
        @foreach (MyActualWebsite.Models.Tag tag in Model)
        {
            <p class="d-block m-2 text-color-heading h-min-content py-2 px-2 bg-secondary  border-2 border-radius-primary border-secondary text-center" style="flex-basis: 90px; flex-grow: 1;">@tag.TagName</p>
        }
    </div>
</div>
