@model MyActualWebsite.Models.TagCatagory

@{
    ViewData["Title"] = "Edit";
}

<h1 class="text-light">Edit</h1>

<h4 class="text-light">TagCatagory</h4>
<hr class="bg-light" />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit" class="labels-light inputs-darker">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="CatagoryId" />
            <div class="form-group">
                <label asp-for="CatagoryName" class="control-label"></label>
                <input asp-for="CatagoryName" class="form-control" />
                <span asp-validation-for="CatagoryName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
