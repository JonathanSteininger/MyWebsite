@model IEnumerable<MyActualWebsite.Models.FAQ>

@{
    ViewData["Title"] = "Index";
}

<h1 class="text-light">Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table text-light" >
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Question)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Answer)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Question)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Answer)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.FAQ_ID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.FAQ_ID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.FAQ_ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
